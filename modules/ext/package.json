  "scripts": {
    "start-chrome": "cross-env BUILD_TARGET=chrome cross-env-shell ./start.sh",
    "start-ff": "cross-env BUILD_TARGET=firefox cross-env-shell ./start.sh",
    "open-ff": "yarn web-ext run -s build",
    "rs": "rm -rf chrome_user_data && NODE_PATH=src react-scripts start",
    "build": "cross-env NODE_PATH=src react-scripts build",
    "build-ff": "cross-env NODE_PATH=src FF_ONLY=true react-scripts build",
    "build-chrome": "cross-env NODE_PATH=src CHROME_ONLY=true react-scripts build",
    "deploy": "env-cmd ./.deployment.env node scripts/deployment/index.js",
    "test": "cross-env NODE_PATH=src NODE_ENV=test npm run test:unit && npm run test:all",
    "eject": "react-scripts eject",
    "start-update-ubo-chrome": "cross-env BUILD_TARGET=chrome rm -rf public/ublock && yarn start-chrome",
    "start-update-ubo-ff": "cross-env BUILD_TARGET=firefox rm -rf public/ublock && yarn start-ff",
    "test-server": "cross-env NODE=true node src/tests/server/index",
    "extract-intl-messages": "cross-env NODE=true node scripts --script extract-intl-messages",
    "import-intl-messages": "cross-env NODE=true node scripts --script import-intl-messages",
    "storybook": "cross-env NODE_PATH=src start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:unit": "cross-env NODE_PATH=src mocha -r jsdom-global/register --require babel_register src/**/test/*.js",
    "test:all": "cross-env WEB_EXT_SESSION_POLLER_INTERVAL=1000 NODE_PATH=src env-cmd -f ./.testing.env mocha --timeout 100000 --require isomorphic-fetch babel_register automated_tests/chrome.tests.js --e2e --logic",
    "test:e2e": "cross-env WEB_EXT_SESSION_POLLER_INTERVAL=1000 NODE_PATH=src env-cmd -f ./.testing.env mocha --timeout 100000 --require isomorphic-fetch babel_register automated_tests/chrome.tests.js --e2e",
    "test:logic": "cross-env WEB_EXT_SESSION_POLLER_INTERVAL=1000 NODE_PATH=src env-cmd -f ./.testing.env mocha --timeout 100000 --require isomorphic-fetch babel_register automated_tests/chrome.tests.js --logic",
    "test:frontend": "jest",
    "lint": "eslint ./src"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      "> 5%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.1",
